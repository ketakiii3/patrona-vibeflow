# Frontend env vars (must be prefixed with VITE_)
VITE_ELEVENLABS_AGENT_ID=your_agent_id_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
VITE_API_URL=
# Clerk auth — get from clerk.com dashboard → API Keys
VITE_CLERK_PUBLISHABLE_KEY=pk_test_...

# Backend env vars (used by Vercel serverless functions and local server)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# Clerk secret key — server only, never expose to frontend
CLERK_SECRET_KEY=sk_test_...

# Supabase (for location tracking persistence)
# Use the anon/public key — NOT the service_role key. Access is enforced by RLS policies.
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here

# Your deployed URL (used in SMS tracking links)
FRONTEND_URL=https://your-app.vercel.app
